// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id       String @id @default(uuid())
  nome     String @unique
  email    String
  whatsapp String
  avatar   String
  password String

  @@map("usuario")
}
